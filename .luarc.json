{
    "$schema": "https://raw.githubusercontent.com/sumneko/vscode-lua/master/setting/schema.json",
    "workspace.maxPreload": 50000,
    "workspace.preloadFileSize": 5000,
    "diagnostics.disable": [
        "need-check-nil",
        "undefined-field",
        "lowercase-global",
        "redundant-return-value",
        "assign-type-mismatch",
        "duplicate-doc-field"
    ],
    "diagnostics.globals": [
        "NotifyOnNewObject",
        "RegisterHook"
    ]
}